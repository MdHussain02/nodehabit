							},
							{
								"key": "Authorization",
								"value": "Bearer {{auth_token}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"name\": \"Morning Exercise\",\n  \"created_time\": \"2024-01-15T10:30:00.000Z\",\n  \"target_time\": \"2024-01-15T07:00:00.000Z\",\n  \"icon_id\": 1,\n  \"repeats\": [0, 1, 2, 3, 4]\n}"
						},
